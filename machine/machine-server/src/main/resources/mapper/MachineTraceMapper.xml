        <result column="operate_emp_id" property="operateEmpId" />
        <result column="comment" property="comment" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, number, imei, paiji_barcode, status_id, receipt_id, time, operate_emp_id, comment
    </sql>

</mapper>
