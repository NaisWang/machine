<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.server.mapper.PriceCombinationMapper">

  <!-- 通用查询映射结果 -->
  <resultMap id="BaseResultMap" type="com.example.server.pojo.PriceCombination">
    <id column="id" property="id"/>
    <result column="grade" property="grade"/>
    <result column="desc" property="desc"/>
    <result column="price1" property="price1"/>
    <result column="price2" property="price2"/>
    <result column="price3" property="price3"/>
  </resultMap>

  <!-- 通用查询结果列 -->
  <sql id="Base_Column_List">
    id, grade,
    excel_desc, price1, price2, price3
  </sql>

</mapper>
